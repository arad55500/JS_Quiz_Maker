{
  "name": "js_quiz_maker",
  "version": "1.1.0",
  "description": "Powered by Node.JS, use a GUI to create quizzes, save them, and transport them as text files that can be imported by the program.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron index.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "Arad Okanin",
  "license": "ISC",
  "build": {
    "appId": "com.example.js_quiz_maker",
    "productName": "JS Quiz Maker",
    "directories": {
      "output": "build"
    },
    "files": [
      "*.js",
      "*.json",
      "uploads/**/*",
      "models/**/*",
      "my_quizzes/**/*",
      "views/**/*" ,
      "node_modules/**/*"
    ]
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.2",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mongodb": "^6.2.0",
    "mongoose": "^7.6.3",
    "multer": "^1.4.5-lts.1",
    "node-notifier": "^10.0.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^8.0.0"
  },
  "devDependencies": {
    "electron": "^27.0.2",
    "electron-builder": "^24.6.4"
  }
}
